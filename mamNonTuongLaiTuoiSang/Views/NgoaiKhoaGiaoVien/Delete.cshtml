@model mamNonTuongLaiTuoiSang.Models.NgoaiKhoaGiaoVien
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Xóa đăng ký ngoại khóa</h1>

<h3>Bạn có chắc chắn muốn xóa đăng ký này không?</h3>
<div>
    <h4>Ngoại khóa - Giáo viên</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdNk)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdNk)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.MaSt)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MaSt)
        </dd>
    </dl>

    <form asp-action="Delete" >
        <input type="hidden" asp-for="IdNk" />
        <input type="hidden" asp-for="MaSt" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index" asp-route-id="@Model.MaSt">Quay lại danh sách</a>
    </form>
</div>
